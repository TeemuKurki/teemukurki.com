  "imports": {
    "@teemukurki/rate-limiter": "jsr:@teemukurki/rate-limiter@^0.0.1",
    "lume/": "https://deno.land/x/lume@v2.0.2/"
  },
  "compilerOptions": {
    "types": [
      "lume/types.ts"
    ]
  }
}
